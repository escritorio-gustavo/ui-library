{
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "emmet.includeLanguages": {
    "javascriptreact": "html",
    "typescriptreact": "html"
  },
  "editor.codeActionsOnSave": {
    "source.organizeImports": false
  },
  "tailwindCSS.experimental.classRegex": [
    ["cva\\(([\\s\\S]*?)\\},?[\\s]*?\\)", "[\"'`]([^\"'`]*).*?[\"'`]"]
  ],
  "files.exclude": {
    "node_modules": true
  }
}
